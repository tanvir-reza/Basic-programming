#include<bits/stdc++.h>
using namespace std;
class myclass{
	char a[20];
	public:
	void show(){cout<<a<<endl;}
	void setdata(char *str){strcpy(a,str);}
};
myclass value(){
	 char a[20];
	 cout<<"Enter Your String : ";
	 cin>>a;
	 myclass x;
	 x.setdata(a);
	 return x;
 }

int main(){
	myclass m;
	m = value();
	m.show();

	return 0;
}
